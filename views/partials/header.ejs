<header class="site-header">
  <% const navSections = typeof sectionVisibility !== 'undefined' && sectionVisibility ? sectionVisibility : {}; %>
  <div class="container nav-wrap">
    <a class="brand" href="/" aria-label="White Lab">
      <span class="brand-mark" aria-hidden="true">
        <img src="/brand/white-lab-logo.svg" alt="" decoding="async" />
      </span>
    </a>

    <nav class="nav-links" id="navigation" aria-label="Разделы сайта" aria-hidden="true">
      <% if (navSections.services !== false) { %><a href="/#services">Услуги</a><% } %>
      <% if (navSections.process !== false) { %><a href="/#process">Процесс</a><% } %>
      <% if (navSections.materials !== false) { %><a href="/#materials">Материалы</a><% } %>
      <% if (navSections.about !== false) { %><a href="/#about">О лаборатории</a><% } %>
      <% if (navSections.gallery !== false) { %><a href="/#gallery">Медиа</a><% } %>
      <% if (navSections.contacts !== false) { %><a href="/#contacts">Контакты</a><% } %>
    </nav>

    <a class="call-btn" href="tel:<%= site?.brand?.phoneValue || '' %>">Запустить кейс</a>
  </div>
</header>
